{{$avatarLink := .RelAvatarLink}}
{{if $avatarLink}}
	<img class="ui avatar gt-vm" src="{{$avatarLink}}" width="32" height="32" alt="{{.FullName}}">
{{else if $.IsTemplate}}
	{{svg "octicon-repo-template" 32}}
{{else if $.IsPrivate}}
	{{svg "octicon-lock" 32}}
{{else if $.IsMirror}}
	{{svg "octicon-mirror" 32}}
{{else if $.IsFork}}
	{{svg "octicon-repo-forked" 32}}
{{else}}
	{{svg "octicon-repo" 32}}
{{end}}
